  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "etcd-service",
    "role": "unit"
  },
  "spec": {
    "ports": [
      {
        "port": 2379,
        "name": "client",
        "targetPort": "client"
      }
    ],
    "selector": {
